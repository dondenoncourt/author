{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@fluentui/react": "8.11.2",
    "@fluentui/react-file-type-icons": "8.0.4",
    "@fluentui/react-hooks": "8.1.2",
    "@fluentui/react-icons": "^0.4.2",
    "@fluentui/react-theme-provider": "^0.18.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
